{
    "name": "ts-poc-mobile",
    "version": "1.0.0",
    "description": "TS POC Mobile",
    "main": "index.js",
    "scripts": {
        "ts-compile": "tsc -p .",
        "ts-compile-watch": "npm run ts-compile -- -w",
        "ts-lint": "tslint -p . -c tslint.json",
        "ts-build": "npm run ts-compile && npm run ts-lint",
        "webpack-build": "webpack",
        "webpack-build-watch": "npm run webpack-build -- -w",
        "cap-copy-watch": "watch 'cap copy' ./src/www --interval=3",
        "predev": "npm run ts-build",
        "dev": "npm run ts-compile-watch & npm run webpack-build-watch",
        "build": "npm run ts-build && npm run webpack-build && cap copy",
        "pretest": "npm run ts-build",
        "test": "mocha --timeout 7200000 --reporter spec --ui tdd --require ts-node/register './test/**/*.test.js' || true"
    },
    "author": "NiviN",
    "license": "ISC",
    "dependencies": {
        "@capacitor/android": "^1.0.0-beta.8",
        "@capacitor/cli": "^1.0.0-beta.8",
        "@capacitor/core": "^1.0.0-beta.8",
        "@capacitor/ios": "^1.0.0-beta.8",
        "@nivinjoseph/n-app": "^1.0.56",
        "@nivinjoseph/n-config": "^1.0.21",
        "@nivinjoseph/n-defensive": "^1.0.19",
        "@nivinjoseph/n-exception": "^1.0.15",
        "@nivinjoseph/n-ext": "^1.0.28",
        "@nivinjoseph/n-ject": "^1.0.22",
        "@nivinjoseph/n-log": "^1.0.5",
        "@nivinjoseph/n-util": "^1.0.19",
        "@nivinjoseph/n-validate": "^1.0.13",
        "@types/jquery": "^3.3.10",
        "@types/mocha": "^5.2.5",
        "@types/node": "^8.10.21",
        "axios": "^0.18.0",
        "jquery": "^3.3.1",
        "mocha": "^5.2.0",
        "moment": "^2.22.2",
        "onsenui": "^2.10.4",
        "ts-node": "^7.0.1",
        "tslint": "^5.11.0",
        "typescript": "^3.1.1",
        "vue-onsenui": "^2.6.1",
        "watch": "^1.0.2"
    }
}
